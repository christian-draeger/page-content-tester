{
  "request": {
    "method": "GET",
    "url": "/referrer"
  },
  "response": {
    "status": 200,
    "bodyFileName": "example3.html",
    "headers": {
      "Content-Type": "text/html; charset=utf-8",
      "User-Agent": "{{request.headers.User-Agent}}",
      "Referer": "{{request.headers.Referer}}",
      "Set-Cookie": "logged_in=no",
      "Custom-Header": "custom value"
    }
  }
}